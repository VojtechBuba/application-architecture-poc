{
    "name": "vojtechbuba/application-architecture-poc",
    "authors": [
        {
            "name": "vojtechbuba",
            "email": "vojtech.buba@peckadesign.cz"
        }
    ],
    "require": {
        "contributte/console": "^0.9.2",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.4",
        "latte/latte": "^2.9",
        "mezzio/mezzio": "^3.9.0",
        "mezzio/mezzio-helpers": "^5.8.0",
        "mezzio/mezzio-laminasrouter": "^3.4.0",
        "nette/application": "^3.1",
        "nette/bootstrap": "^3.1",
        "nette/caching": "^3.1",
        "nette/database": "^3.1",
        "nette/di": "^3.0",
        "nette/finder": "^2.5",
        "nette/forms": "^3.1",
        "nette/http": "^3.1",
        "nette/mail": "^3.1",
        "nette/robot-loader": "^3.3",
        "nette/schema": "^1.2",
        "nette/security": "^3.1",
        "nette/utils": "^3.2",
        "nettrine/orm": "^0.8.3",
        "php": ">= 7.4",
        "ramsey/uuid": "^4.2",
        "tracy/tracy": "^2.8"
    },
    "require-dev": {
        "nette/tester": "^2.3",
        "phpunit/phpunit": "^9.5",
        "symfony/thanks": "^1",
        "symplify/monorepo-builder": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "Pd\\StorageAdmin\\": "packages/storage-admin/app/",
            "Pd\\StorageSDK\\": "packages/storage-sdk/src/",
            "Pd\\Storage\\": "packages/storage/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PdStorage\\Tests\\": "tests/unit",
            "Pd\\Storage\\Tests\\": "packages/storage/tests"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/VojtechBuba/storage-sdk"
        }
    ],
    "replace": {
        "pd/storage": "1.6.0",
        "pd/storage-admin": "1.6.0",
        "pd/storage-sdk": "1.6.0"
    },
    "config": {
        "allow-plugins": {
            "symfony/thanks": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
