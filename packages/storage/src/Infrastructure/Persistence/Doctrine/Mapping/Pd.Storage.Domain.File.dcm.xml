<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Pd\Storage\Domain\File" table="storage_file">
        <id name="id" type="storage.fileId"/>
        <field name="path" type="storage.path"/>
        <field name="size" type="storage.byte"/>
        <field name="tenantId" type="storage.tenantId"/>
        <field name="extension" type="storage.extension"/>
        <field name="created" type="datetime_immutable" />
    </entity>
</doctrine-mapping>
